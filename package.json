{
  "name": "karma-rollup-plugin",
  "version": "0.2.1",
  "description": "A Karma preprocessor plugin to offer seamless integration with Rollup",
  "main": "dist/rollup-plugin-karma.js",
  "author": "KFlash",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src test/*.js",
    "build": "rm -rf dist && rollup -c",
    "pretest": "npm run build",
    "test": "mocha",
    "posttest": "npm run lint",
    "prepublish": "npm test"
  },
  "files": [
    "src",
    "dist",
    "rollup.conf.js"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Kflash/karma-rollup-plugin.git"
  },
  "keywords": [
    "karma",
    "karma-plugin",
    "karma-preprocessor",
    "modules",
    "es2016",
    "rollup",
    "buble",
    "preprocessor"
  ],
  "bugs": {
    "url": "https://github.com/Kflash/karma-rollup-plugin/issues"
  },
  "homepage": "https://github.com/Kflash/karma-rollup-plugin",
  "dependencies": {
    "debounce": "^1.0.0",
    "rollup": "^0.34.1"
  },
  "devDependencies": {
    "buble": "^0.12.5",
    "chai": "^3.5.0",
    "eslint": "^3.2.1",
    "karma": "^1.1.2",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "mocha": "^2.5.3",
    "object-assign": "^4.1.0",
    "phantomjs-prebuilt": "^2.1.9",
    "rollup-plugin-buble": "^0.12.1"
  }
}
