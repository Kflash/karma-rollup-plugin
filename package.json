{
  "name": "karma-rollup-plugin",
  "version": "0.0.1",
  "description": "A Karma preprocessor plugin to offer seamless integration with Rollup",
  "main": "dist/karma-rollup-plugin.js",
  "author": "KFlash",
  "license": "MIT",
  "jsnext:main": "dist/karma-rollup-plugin.es.js",
  "scripts": {
    "test": "mocha test/specs --recursive --compilers js:babel-register",
    "build": "rollup -c",
    "prebuild": "rm -rf dist/*",
    "pretest": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Kflash/karma-rollup-plugin.git"
  },
  "keywords": [
    "karma",
    "karma-plugin",
    "karma-preprocessor",
    "modules",
    "es2016",
    "rollup",
    "buble",
    "preprocessor"
  ],
  "bugs": {
    "url": "https://github.com/Kflash/karma-rollup-plugin/issues"
  },
  "homepage": "https://github.com/Kflash/karma-rollup-plugin",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "debounce": "^1.0.0",
    "karma": "^1.1.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "mocha": "^2.5.3",
    "phantomjs-prebuilt": "^2.1.7",
    "rollup": "^0.33.1",
    "rollup-plugin-node-resolve": "^1.7.1",
    "rollup-plugin-multi-entry": "^2.0.0",
    "rollup-plugin-buble": "^0.12.1"
  }
}
